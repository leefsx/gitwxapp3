  .topbar-img{
    width: 87rpx;
    height: 86rpx;
    border-radius: 8rpx;
    text-align: center;
    line-height: 86rpx;
  }
  .topbar-img image{
    width: 64rpx;
    height: 64rpx;
    margin-top: 10rpx;
  }
  .page-title{
    height: 100rpx;
    line-height: 100rpx;
    padding: 0 50rpx;
    color: #3d3d3d;
    font-size: 36rpx;
  }
  .page-title text:last-child{
    color: #b5b5b5;
    margin-left: 15rpx;
  }