<import src="../../common/nav.wxml" />
<import src="../../common/bar.wxml" />




<view class="main">
    <template is="nav" data="{{config,page_title,page_title_en}}" />
    <template is="bar" data="{{topNavs}}" />
    <view class="us-article">
        <view class="us-img"><image wx:if="{{config.aboutus_img}}" src="{{config.aboutus_img}}"></image></view>
        <view class="us-content">
                {{config.aboutus_desc}}
        </view>
    </view>
</view>
